[project]
	description = this is a test repository to see how it goes with gerrithub
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Francois Coulombe (fcoulombegoogle)
[access "refs/for/refs/*"]
	read = group user/Francois Coulombe (fcoulombegoogle)
	push = group user/Francois Coulombe (fcoulombegoogle)
	label-Code-Review = -2..+2 group user/Francois Coulombe (fcoulombegoogle)
	label-Verified = -1..+1 group user/Francois Coulombe (fcoulombegoogle)
	rebase = group user/Francois Coulombe (fcoulombegoogle)
	removeReviewer = group user/Francois Coulombe (fcoulombegoogle)
	submit = group user/Francois Coulombe (fcoulombegoogle)
[access "refs/heads/*"]
	read = group user/Francois Coulombe (fcoulombegoogle)
	create = group user/Francois Coulombe (fcoulombegoogle)
	push = +force group user/Francois Coulombe (fcoulombegoogle)
	pushMerge = group user/Francois Coulombe (fcoulombegoogle)
[access "refs/tags/*"]
	push = +force group user/Francois Coulombe (fcoulombegoogle)
	pushTag = group user/Francois Coulombe (fcoulombegoogle)
	pushSignedTag = group user/Francois Coulombe (fcoulombegoogle)
